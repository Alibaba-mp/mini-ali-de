{
  "name": "@de2/helpers",
  "version": "0.1.0-beta.1",
  "description": "helpers for de",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "files": [
    "esm",
    "lib"
  ],
  "scripts": {
    "dev": "npm run dev:cjs",
    "dev:cjs": "tsc -p ./tsconfig.json --watch",
    "dev:esm": "tsc -p ./tsconfig.esm.json --watch",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p ./tsconfig.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "lint": "tslint --project ./tsconfig.json",
    "lint:fix": "tslint --fix --project ./tsconfig.json",
    "prepublishOnly": "npm run build",
    "test": "BABEL_ENV=test mocha tests/* --no-deprecation --compilers js:babel-register",
    "cov": "BABEL_ENV=test babel-node ../../node_modules/istanbul/lib/cli.js cover _mocha tests/* -- --no-deprecation --compilers js:babel-register",
    "cov:report": "npm run cov && cat ./coverage/lcov.info | ../../node_modules/coveralls/bin/coveralls.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@de2/shared": "0.1.0-beta.1"
  },
  "gitHead": "baeba9ec0f59aa4e66558ebc5209ca37db759546"
}
